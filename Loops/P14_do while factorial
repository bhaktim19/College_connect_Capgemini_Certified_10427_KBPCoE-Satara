package com.edu.loop;

import java.util.Scanner;

public class DoFact {

	public static void main(String[] args) {
		int a,n,fact=1;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter anu number :");
		n=sc.nextInt();
		
		a=1;
		do {
			fact=fact*a;
			a++;
		}while(a<=n);
		
		System.out.println("Factorial ="+fact);
		

	}
}

